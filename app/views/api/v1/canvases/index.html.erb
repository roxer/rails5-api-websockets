<p id="notice"><%= notice %></p>

<h1>Api V1 Canvases</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Diagram</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_canvases.each do |api_v1_canvas| %>
      <tr>
        <td><%= api_v1_canvas.name %></td>
        <td><%= api_v1_canvas.diagram %></td>
        <td><%= link_to 'Show', api_v1_canvas %></td>
        <td><%= link_to 'Edit', edit_api_v1_canvas_path(api_v1_canvas) %></td>
        <td><%= link_to 'Destroy', api_v1_canvas, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Canvas', new_api_v1_canvas_path %>
