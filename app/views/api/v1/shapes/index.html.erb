<p id="notice"><%= notice %></p>

<h1>Api V1 Shapes</h1>

<table>
  <thead>
    <tr>
      <th>Label</th>
      <th>Type</th>
      <th>Canvas</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_shapes.each do |api_v1_shape| %>
      <tr>
        <td><%= api_v1_shape.label %></td>
        <td><%= api_v1_shape.shape_type %></td>
        <td><%= api_v1_shape.canvas %></td>
        <td><%= link_to 'Show', api_v1_shape %></td>
        <td><%= link_to 'Edit', edit_api_v1_shape_path(api_v1_shape) %></td>
        <td><%= link_to 'Destroy', api_v1_shape, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Shape', new_api_v1_shape_path %>
